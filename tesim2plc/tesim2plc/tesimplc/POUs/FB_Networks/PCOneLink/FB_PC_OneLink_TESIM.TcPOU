<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.0.36">
  <POU Name="FB_PC_OneLink_TESIM" Id="{3bd205c6-e14a-4957-98ff-aed794bd8894}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_PC_OneLink_TESIM EXTENDS FB_PC_OneLink_Base
VAR_INPUT
